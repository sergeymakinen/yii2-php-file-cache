{
    "name": "sergeymakinen/yii2-php-file-cache",
    "homepage": "https://github.com/sergeymakinen/yii2-php-file-cache",
    "description": "Yii 2 PHP file cache",
    "keywords": ["yii2-php-file-cache", "yii2", "yii", "php", "file", "cache", "caching"],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/sergeymakinen/yii2-php-file-cache/issues",
        "source": "https://github.com/sergeymakinen/yii2-php-file-cache"
    },
    "authors": [
        {
            "name": "Sergey Makinen",
            "email": "sergey@makinen.ru",
            "homepage": "https://makinen.ru"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "~2.0.1"
    },
    "require-dev": {
        "sergeymakinen/yii2-tests": "^1.0"
    },
    "autoload": {
        "classmap": [
            "src/PhpFileCache.php",
            "src/ValueWithBootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "sergeymakinen\\tests\\caching\\": "tests"
        }
    }
}
